{
  "name": "chat-app-backend",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "dev": "nodemon index.ts",
    "migrations": "concurrently \"npm:migrations:development\" \"npm:migrations:test\"",
    "migrations:development": "npx sequelize-cli db:migrate --env development",
    "migrations:test": "npx sequelize-cli db:migrate --env test",
    "migrations:undo": "concurrently \"npm:migrations:development:undo\" \"npm:migrations:test:undo\"",
    "migrations:development:undo": "npx sequelize-cli db:migrate:undo --env development",
    "migrations:test:undo": "npx sequelize-cli db:migrate:undo --env test",
    "test": "cross-env NODE_ENV=test jest --detectOpenHandles"
  },
  "jest": {
    "testEnvironment": "node",
    "coveragePathIgnorePatterns": [
      "/node_modules"
    ]
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "bcrypt": "^5.0.0",
    "cors": "^2.8.5",
    "cross-env": "^7.0.3",
    "express": "^4.17.1",
    "jsonwebtoken": "^8.5.1",
    "pg": "^8.4.2",
    "pg-hstore": "^2.3.3",
    "sequelize": "^6.3.5"
  },
  "devDependencies": {
    "@babel/preset-typescript": "^7.12.7",
    "@types/bcrypt": "^3.0.0",
    "@types/bluebird": "^3.5.33",
    "@types/cors": "^2.8.10",
    "@types/express": "^4.17.8",
    "@types/jest": "^26.0.20",
    "@types/jsonwebtoken": "^8.5.0",
    "@types/node": "^14.14.6",
    "@types/sequelize": "^4.28.9",
    "@types/supertest": "^2.0.10",
    "@types/validator": "^13.1.0",
    "@typescript-eslint/eslint-plugin": "^4.6.1",
    "@typescript-eslint/parser": "^4.6.1",
    "concurrently": "^5.3.0",
    "eslint": "^7.23.0",
    "eslint-config-airbnb-base": "^14.2.1",
    "eslint-config-prettier": "^8.1.0",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-prettier": "^3.3.1",
    "jest": "^26.6.3",
    "nodemon": "^2.0.6",
    "prettier": "2.1.2",
    "sequelize-cli": "^6.2.0",
    "supertest": "^6.1.3",
    "ts-jest": "^26.5.0",
    "ts-node": "^9.0.0",
    "typescript": "^4.0.5"
  }
}
